<div class="row">
  <div class="col-6 col-md-4 ml-4">
    <div class="row justfy-content-right">
      <div class="col-md-4"></div>
      <div class="card shadow" style="width: 20rem;">
        <div class="inner">
          <img src="styles/images/dog1.jpg" class="card-img-top" alt="card_image cap">
        </div>
        <div class="card-body">
          <h5 class="card-title">Walk a DOGO</h5>
          <p class="card-text">Some quick example text to build on the card title and make up the bulk of the
            card's content.</p>
          <a href="/customer" class="btn btn-success">Schedule a Walk!</a>
        </div>
      </div>
    </div>
  </div>
  <div class="col-6 col-md-4 ml-4">
    <div class="row justfy-content-left">
      <div class="col-md-4"></div>
      <div class="card shadow" style="width: 20rem; height: 370px;">
        <div class="inner">
          <img src="styles/images/dog1.jpg" class="card-img-top" alt="card_image cap">
        </div>
        <div class="card-body">
          <h5 class="card-title">Become a Walker today!</h5>
          <p class="card-text">Some quick example text </p>
          <a href="/walker" class="btn btn-success">Become A Dog Walker!</a>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
</div>

<ul id="example-list" class="list-group">
  {{#each examples}}
  <li data-id="{{this.id}}" class="list-group-item">
    <a href="example/{{this.id}}">{{this.text}}</a>
    <button class="btn btn-danger float-right delete">ï½˜</button>
  </li>
  {{/each}}
</ul>
</div>
</div>
